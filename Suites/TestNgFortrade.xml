<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Fortrade Pages">
    <listeners>
        <listener class-name="listeners.ScreenshotListener"/>
        <listener class-name="listeners.Transform"/>
    </listeners>
    <test name="Links redirection - FSC regulation">
        <parameter name="countryCode" value="381"/>
        <parameter name="regulation" value="FSC"/>
        <parameter name="tag" value="&amp;tg=skip&amp;G_GEO=2051&amp;regulation=FSC"/>
        <classes>
            <class name="TestsFortrade.new_dark_2024_dlp">
                <methods>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="sameFNameAndLName"/>
                    <include name="checkingLogoClickability"/>
                    <include name="checkForCountryCodeErrorMessage"/>
                    <include name="alreadyRegisteredAccountTest"/>
                </methods>
            </class>
        </classes>
    </test>
