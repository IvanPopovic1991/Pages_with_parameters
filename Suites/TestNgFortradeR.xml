<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="FortradeR Pages">
    <listeners>
        <listener class-name="listeners.ScreenshotListener"/>
        <listener class-name="listeners.Transform"/>
    </listeners>
    <test name="Demo account is not registered - FortradeR">
        <classes>
            <class name="TestsFortradeR.new_dark_2024_dlp">
                <methods>
                    <include name="unsuccessfullyDemoAccountRegistration"/>
                    <include name="emptyDemoAccountRegistration"/>
                    <include name="alreadyRegisteredAccountTest"/>
                    <include name="sameFNameAndLName"/>
                    <include name="checkingForLogoClickability"/>
                    <include name="checkForCountryCodeErrorMessage"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>